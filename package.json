    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "prepublishOnly": "npm run build",
