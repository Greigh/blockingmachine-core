  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "@types/progress": "2.0.7",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.14.0",
